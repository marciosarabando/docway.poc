@{
    ViewData["Title"] = "Lista de m√©dicos obtida da API";
}


<h1>@ViewData["Title"]</h1>
<br>
@{
    if(ViewBag.medicos != null)
    {
        for (int i = 0; i < ViewBag.medicos.Count; i++)
        {
            <p>@ViewBag.medicos[i].Id - @ViewBag.medicos[i].Nome - @ViewBag.medicos[i].Especialidade</p>
        }
    }
    else
    {
        <p>Token Expirado</p>
    }
}
